package com.ekinoksyazilim.etkk.prototype.comm;

import java.net.InetSocketAddress;

public class Envelop <T> {

	private T message;
	private InetSocketAddress address;
	
	public Envelop(T message, InetSocketAddress to) {
		
		this.message = message;
		this.address = to;
	}
	
	public T getMessage() {
		
		return message;
	}
	
	public InetSocketAddress getTo() {
		
		return address;
	}
}
